{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"globals": {
		"describe": true,
		"it": true,
		"expect": true
	},
	"plugins": [
		"import",
		"jest"
	],
	"extends": "eslint:recommended",
	"rules": {
		"no-console": 1,
		"comma-dangle": [
			"warn",
			"always-multiline"
		],
		"comma-spacing": [
			1,
			{
				"before": false,
				"after": true
			}
		],
		"eol-last": 1,
		"indent": [
			"warn",
			"tab"
		],
		"key-spacing": 1,
		"no-multi-spaces": 1,
		"keyword-spacing": 1,
		"no-case-declarations": 0,
		"no-unused-vars": 2,
		"no-trailing-spaces": 1,
		"object-curly-spacing": [
			1,
			"always"
		],
		"quotes": [
			"warn",
			"single",
			{
				"avoidEscape": true
			}
		],
		"semi": 1,
		"space-before-function-paren": 1
	}
}
